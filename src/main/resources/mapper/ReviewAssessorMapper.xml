<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.furp.mapper.ReviewAssessorMapper">

    <delete id="deleteByAnnualReviewIds">
        DELETE FROM review_assessor WHERE annual_review_id IN
        <foreach item="item" collection="reviewIds" open="(" separator="," close=")">
            #{item}
        </foreach>
    </delete>

</mapper>