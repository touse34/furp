<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.furp.mapper.PhdSkillMapper">

    <insert id="insertPhdSkill">
        INSERT INTO phd_skill(phd_id, skill_id)
        VALUES
        <foreach item="item" collection="skillIds" separator=",">
            (#{phdId}, #{item})
        </foreach>
    </insert>

</mapper>